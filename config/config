export PLUGIN_INGRESS_NAME=ingress

export PLUGIN_INGRESS_DATA=$(plugin::get_app_data_path $PLUGIN_INGRESS_NAME)
export PLUGIN_INGRESS_LOGS=$(plugin::get_app_logs_path $PLUGIN_INGRESS_NAME)
export PLUGIN_INGRESS_CONFIG=$(plugin::get_app_configs_path $PLUGIN_INGRESS_NAME)/config

export PLUGIN_INGRESS_SERVICE_NAME=$PLUGIN_INGRESS_NAME

export PLUGIN_INGRESS_SERVICE_CONFIG_DIR=$PLUGIN_INGRESS_CONFIG/service
export PLUGIN_INGRESS_SERVICE_DATA_DIR=$PLUGIN_INGRESS_DATA/service

# @TODO for service ingress
export SERVICE_INGRESS_CONFIG_DIR=$PLUGIN_INGRESS_SERVICE_CONFIG_DIR
export SERVICE_INGRESS_DATA_DIR=$PLUGIN_INGRESS_SERVICE_DATA_DIR
